<VisualStudioPropertySheet
    ProjectType="Visual C++"
    Version="8.00"
    Name="fila">

    <!--
    =====================================================================
    =====================================================================
    -->

    <UserMacro
        Name="FILA_GROUP"
        Value=""
    />
    <UserMacro
        Name="FILA_NAME"
        Value="fila"
    />
    <UserMacro
        Name="FILA_VERSION"
        Value=""
    />

    <!--
    =====================================================================
    =====================================================================
    -->

    <UserMacro
        Name="FILA_DIR"
        Value="$(FILA_GROUP)$(FILA_NAME)$(FILA_VERSION)"
    />
    <UserMacro
        Name="FILA_SOURCE_DIR"
        Value="$(SOURCE_ROOT_DIR)/$(FILA_DIR)/source"
    />
    <UserMacro
        Name="FILA_BUILD_DIR"
        Value="$(SOURCE_BUILD_DIR)/$(FILA_DIR)/build"
    />

    <!--
    =====================================================================
    =====================================================================
    -->

    <UserMacro
        Name="FILA_BUILD"
        Value="$(FILA_BUILD_DIR)/$(PlatformName)/$(VCVersion)/$(ConfigurationName)"
    />
    <UserMacro
        Name="FILA_OBJ"
        Value="$(FILA_BUILD)/obj/$(ProjectName)"
    />
    <UserMacro
        Name="FILA_LIB"
        Value="$(FILA_BUILD)/lib"
    />
    <UserMacro
        Name="FILA_BIN"
        Value="$(FILA_BUILD)/bin"
    />

    <!--
    =====================================================================
    =====================================================================
    -->

    <UserMacro
        Name="FILA_DEFINES"
        Value=""
    />
    <UserMacro
        Name="FILA_DEBUG_DEFINES"
        Value="$(FILA_DEFINES)"
    />
    <UserMacro
        Name="FILA_RELEASE_DEFINES"
        Value="$(FILA_DEFINES)"
    />

    <!--
    =====================================================================
    =====================================================================
    -->
  
    <UserMacro
        Name="FILA_INCLUDE_DIRS"
        Value="$(FILA_SOURCE_DIR)"
    />
    <UserMacro
        Name="FILA_LIB_DIRS"
        Value="$(FILA_LIB)"
    />
    <UserMacro
        Name="FILA_LIBS"
        Value="libfila.lib"
    />


    <!--
    =====================================================================
    =====================================================================
    -->

</VisualStudioPropertySheet>
<!--
=====================================================================
$(FILA_BIN)
$(FILA_LIB)
$(FILA_OBJ)
